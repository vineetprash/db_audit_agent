{
  "name": "audit-system",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run db:push:safe && next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "db:push": "prisma db push --skip-generate && prisma generate",
    "db:push:safe": "prisma db push --skip-generate --accept-data-loss || true && prisma generate",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@tailwindcss/postcss": "^4.1.16",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.30.0",
    "next": "^14.0.4",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.46",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.5.6",
    "prisma": "^5.7.0",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.3.3"
  }
}
